{
  "name": "green-cycle-bank",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "turbo": "^1.10.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@sentry/nextjs": "^9"
  }
}
